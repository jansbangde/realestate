body
	background #EFEFF4	
*
	margin 0
	padding 0
.search
	width 100%
	height 50px
	background white
	input
		height 36px	
		border none
		background #EFEFF4
		width 70%
		border-radius 5px
		margin-top 7px
		margin-left 3%
		padding-left 30px
		outline none
	.search_text
		font-size 14px
		margin-left 14px
	.search_pic
		display block
		position absolute
		width 20px
		height 20px
		top 15px
		left 5%		
		background url('../../img/home_page/nav_icon_search@2x.png') no-repeat left
		background-size 80%
.search_prompt
	width 100%
	font-size 12px		
	color #888
	overflow hidden
	.search_prompt_bt1
		float left	
		margin-left 5%
		line-height 40px
	.search_prompt_bt2
		float right	
		margin-right 5%	
		line-height 40px
.search_bottom
	width 100%
	height 550px
	background white	
	
		
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	